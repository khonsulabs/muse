$case(
    basic_struct: r#"
        struct Foo {
            field;
            pub fn bar {
                [] => 1,
                [self] => self.field,
            };
        };

        $assert(Foo.bar() == 1);

        let instance = new Foo {
            field: 42,
        };
        $assert(instance.bar() == 42);
    "#
);
