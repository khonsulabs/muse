format: {
    src: r#"
        let name = "world";
        f"hello, ${ name }!"
    "#,
    output: String("hello, world!"),
}

format_escape: {
    src: r#"
        f"hello, \${ name }! $"
    "#,
    output: String("hello, ${ name }! $"),
}